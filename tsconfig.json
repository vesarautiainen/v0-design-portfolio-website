{
	"compilerOptions": {
		"moduleResolution": "bundler",
		"target": "ES2020",
		"useDefineForModuleExport": true,
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"types": ["svelte"],
		"resolveJsonModule": true,
		"esModuleInterop": true
	},
	"include": ["src/**/*.d.ts", "src/**/*.ts", "src/**/*.svelte", "src/routes/+layout.ts", "src/routes/+layout.ts"]
}
